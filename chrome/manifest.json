{
  "manifest_version": 2,
  "name": "VineVolumeFixer",
  "version": "1.0.3",
  "description": "Adjusts vine video volume to a normal level & adds volume controls",
  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "matches": [
      "*://vine.co/*",
      "*://*.vine.co/*"
    ],
    "js": ["pagemod.js"],
    "all_frames": true
  }],
  "options_ui": {
    "page": "settings.html",
    "chrome_style": true
  }
}
